Options All -Indexes

RewriteEngine On

RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteCond %{REQUEST_URI} !/$
RewriteRule ^(.*)$ $1\.php


RewriteRule ^Connecter$ vue/login [L]
RewriteRule ^Home$ vue/accueilMaster [L]
RewriteRule ^Abm$ mba/vue/login [L]
RewriteRule ^Accueil_mba$ mba/vue/accueil [L]
RewriteRule ^Register$ vue/inscription [L]


